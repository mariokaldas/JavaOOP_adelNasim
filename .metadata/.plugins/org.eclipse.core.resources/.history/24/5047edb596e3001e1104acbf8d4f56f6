package passingObjects2methods;

public class Main {
	
	/*
	 * no pointers in java
	 * no call by reference for permeative data-types (int float double)
	 * */
	static void fn(int n) { 
		n++;
	}
	
	public static void main(String args[]) {
		
		int number =1;
		fn(number); //pass by value
		System.out.println(number);
		
	}
		
}
